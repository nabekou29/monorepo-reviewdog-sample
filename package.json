{
  "name": "monorepo-reviewdog-sample",
  "private": true,
  "scripts": {
    "lint": "pnpm -r lint",
    "lint:reviewdog": "pnpm -r --parallel lint:reviewdog",
    "format": "prettier --write \"**/*.{js,ts,mjs,jsx,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,mjs,jsx,tsx,json,md}\""
  },
  "devDependencies": {
    "eslint": "^9.23.0",
    "eslint-formatter-rdjson": "^1.0.6",
    "prettier": "^3.5.3"
  },
  "packageManager": "pnpm@10.6.1"
}
